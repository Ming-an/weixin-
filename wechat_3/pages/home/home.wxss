/* pages/home/home.wxss */
/*轮播图样式*/
.bannerList{
  width: 100rpx;
  border-radius: 100rpx;
}
/**九宫格样式**/
.grid{
  display: flex;
  flex-wrap: wrap;
}

.grid .grid-items{
  display: flex;
  flex-direction: column;
  width: 33%;
  align-items: center;
  justify-content: space-around;
  border-left: 1rpx solid #efefef;
  border-bottom:1rpx solid #efefef;
  border-radius: 10px;
}

.grid .grid-items image{
  width: 180rpx;
  height: 150rpx;
}
.goods-item{
  display: flex;
  flex-direction: column;
  width: 20%;
  align-items: center;
  justify-content: space-around;
  border-left: 1rpx solid #efefef;
  border-bottom:1rpx solid #efefef;
  border-radius: 10px;
}

.navList{
  display: flex;
  justify-content: space-around;
}
.navList .navList-item image{
  width: 128rpx;
  height: 140rpx;
}
/*楼层样式*/
.floor-title image{
  display: flex;
  width: 100%;
  height: 100rpx;
}

.index_floor .floor_group .floor-title image{
  width: 100%;
}

.index_floor .floor_group .floor_list navigator{
  float: left;
  width: 33.33%;
}
.index_floor .floor_group .floor_list navigator image{
  width: 100%;
}